{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 5 */1 * * *"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "df-api",
      "collectionName": "videos",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDbConnectionString"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "df-api",
      "collectionName": "videos",
      "connectionStringSetting": "CosmosDbConnectionString",
      "sqlQuery": "SELECT * FROM c ORDER BY c.created DESC OFFSET 0 LIMIT 10"
    }
  ],
  "scriptFile": "../dist/scrapeForNewVideos/index.js"
}
